/**index.wxss**/
.t_red{
  color: #fff;
}
.t_blue{
  color: #8B7765;
}
.calendar{

  margin: 0 1vw;
}
.time{
  padding: 16rpx 20rpx;
  background-color: rgb(156, 154, 149);
  display: flex;
}
.time view{
  flex: 1;
  font-size: 30rpx;

}
.time view text{
  font-size: 38rpx;
}
.weekName{
   background-color: #8B7765;
  width: 100%;
  display: flex;
  padding: 16rpx 0;
}
.weekName view{
  flex: 1;
  text-align: center;
}
.sunday,.saturday{
  color: #fff;
}
.week{
  width: 100%;
}
.week view{
  width: 14.2%;
  height: 40rpx;
  line-height: 40rpx;
  display: inline-block; 
  margin: 20rpx 0;
  text-align: center;
}
.week view text{
  width: 100%;
  height: 100%;
  display: inline-block;
}
.calendarSign{
  margin: 20rpx;
}
.vivid_wrap{
  width: 100%;
}
.vivid_title{
  width: 100%;
  text-align: center; 
  font-size: 36rpx;
  font-weight: 600;
  color: #fff;
  background-color:#8B7765;
}
.vivid_content{
  display:flex;                    
  justify-content: center;        
}
.vivid_content image{
  width:70%;     
}

page{
  padding-top:10rpx;
}